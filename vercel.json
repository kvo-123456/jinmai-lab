{
  "version": 2,
  "installCommand": "pnpm install",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "pnpm build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "^/(?!assets|chunks|entries|icons|images|manifest\.webmanifest|sw\.js|workbox-|registerSW\.js|AR|particle-system\.js).*",
      "destination": "/index.html"
    }
  ]
}